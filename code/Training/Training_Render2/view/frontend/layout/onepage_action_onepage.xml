<?xml version="1.0"?>
<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceContainer name="columns">
            <block class="Magento\Framework\View\Element\Html\Links" name="custom_links">
                <argument name="label" xsi:type="string" translate="true">Custom_link</argument>
            </block>
        </referenceContainer>

        <referenceContainer name="render" remove="true" />
    </body>
</page>